<h1> Welcome To profile Page<h1>
<%= link_to 'Logout', session_path(@user), method: :delete %>

<%= form_for @user do |f| %>
  Email: <%= f.text_field :email, disabled: true %>
  Username: <%= f.text_field :username, value: @user.username %>
  Password: <%= f.password_field :password %>
  <%= f.submit 'Submit' %>
<% end %>

